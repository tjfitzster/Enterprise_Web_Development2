<div class="uk-width-2-3@m  uk-card uk-card-default uk-padding-small uk-visible@m">
  <div class="uk-grid">
    <div class="uk-width-1-6@m">
      <img src="/images/POI1.png" width="40" alt="img" />
    </div>
    <div class="uk-width-expand@m uk-text-left">
      <div class="title">
        {{title}}
      </div>
      <div class="uk-text-muted uk-text-small">
        {{subTitle}}
      </div>
    </div>
    <div class="uk-card uk-card-small uk-text-center uk-padding-remove">
      <a target="”_blank”" href="/">
        <div>
          <i
            class="fas fa-map-marked-alt fa-3x"
            style="color:rgb(95, 96, 173)"
            title="Main POI Screen"
            pos="bottom"
            uk-tooltip
          ></i>
        </div>
        <div class="uk-width-expand@m uk-text-left">
          <div class="uk-text-muted uk-text-center" style="font-size:xx-small">POI</div>
        </div>
      </a>
    </div>
  </div>
</div>